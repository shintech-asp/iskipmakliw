@{
    ViewData["Title"] = "Dashboard";
}
@model UserDetails;
@if(Model.Status == "Pending" || Model.Status == "For Approval")
{
    <div class="alert alert-warning my-5">
        @if(Model.Status == "Pending")
        {
            <p>
                <strong>Warning!</strong> Your account is currently @Model.Status.ToLower() for your payment. Please contact support for further assistance.
            </p>
            <button class="btn btn-primary">Pay now!</button>
        }
        else
        {
            <p>
                <strong>Warning!</strong> Your account is currently waiting @Model.Status.ToLower(). Please contact support for further assistance.
            </p>
        }
    </div>
}
